{
  "name": "example",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "craco build",
    "start": "craco start",
    "prepare": "run-s build && mkdirp reports/unit/html reports/lint",
    "format": "prettier --write .",
    "test": "run-s test:unit test:lint test:build test:format",
    "test:build": "run-s build",
    "test:lint": "eslint . --format html --output-file reports/lint/report.html",
    "test:unit": "craco test --watchAll=false",
    "test:watch": "craco test --env=jsdom",
    "test:format": "prettier --check ."
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@craco/craco": "^7.0.0",
    "@craco/types": "^7.0.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/fira-code": "^4.5.12",
    "@fontsource/roboto": "^4.5.8",
    "@jest/types": "^29.3.1",
    "@mui/icons-material": "^5.10.16",
    "@mui/material": "^5.10.16",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/is-hotkey": "^0.1.7",
    "@types/node": "^18.11.10",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-react": "^12.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "jest-html-reporters": "^3.0.11",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.4",
    "react-scripts": "^5.0.1",
    "react-smartkeys": "link:../react-smartkeys",
    "typescript": "4.8.4"
  }
}
